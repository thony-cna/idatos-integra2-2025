<?xml version="1.0" encoding="UTF-8"?>
<prov:document xmlns:prov="http://www.w3.org/ns/prov#"
               xmlns:dcterms="http://purl.org/dc/terms/"
               xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
               xmlns:ex="http://example.org/">

    <!-- Entidad principal -->
    <prov:entity prov:id="ex:l1books">
        <prov:label>Tabla l1books</prov:label>
        <prov:type>prov:Collection</prov:type>
        <dcterms:description>
            Tabla consolidada con metadatos de libros y reseñas, creada a partir del dataset
            Amazon Books Reviews. Representa la versión original de los datos antes de la normalización.
        </dcterms:description>
        <dcterms:source>https://www.kaggle.com/datasets/mohamedbakhet/amazon-books-reviews</dcterms:source>
        <dcterms:date>2025-09-02T18:39:21</dcterms:date>


        <!-- Miembros (columnas) -->
        <prov:hadMember prov:entity="ex:title"/>
        <prov:hadMember prov:entity="ex:description"/>
        <prov:hadMember prov:entity="ex:authors"/>
        <prov:hadMember prov:entity="ex:image"/>
        <prov:hadMember prov:entity="ex:previewlink"/>
        <prov:hadMember prov:entity="ex:publisher"/>
        <prov:hadMember prov:entity="ex:publishedDate"/>
        <prov:hadMember prov:entity="ex:infolink"/>
        <prov:hadMember prov:entity="ex:categories"/>
        <prov:hadMember prov:entity="ex:ratingscount"/>
    </prov:entity>

    <!-- Columnas -->
    <prov:entity prov:id="ex:title">
        <prov:label>title</prov:label>
        <prov:type>xsd:string</prov:type>
        <dcterms:description>Título del libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:description">
        <prov:label>description</prov:label>
        <prov:type>xsd:string</prov:type>
        <dcterms:description>Sinopsis o resumen del libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:authors">
        <prov:label>authors</prov:label>
        <prov:type>xsd:string</prov:type>
        <dcterms:description>Lista de autores asociados al libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:image">
        <prov:label>image</prov:label>
        <prov:type>xsd:anyURI</prov:type>
        <dcterms:description>URL de la imagen del libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:previewlink">
        <prov:label>previewlink</prov:label>
        <prov:type>xsd:anyURI</prov:type>
        <dcterms:description>Enlace de vista previa del libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:publisher">
        <prov:label>publisher</prov:label>
        <prov:type>xsd:string</prov:type>
        <dcterms:description>Editorial que publicó el libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:publishedDate">
        <prov:label>publishedDate</prov:label>
        <prov:type>xsd:gYear</prov:type>
        <dcterms:description>Año de publicación del libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:infolink">
        <prov:label>infolink</prov:label>
        <prov:type>xsd:anyURI</prov:type>
        <dcterms:description>Enlace con información adicional del libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:categories">
        <prov:label>categories</prov:label>
        <prov:type>xsd:string</prov:type>
        <dcterms:description>Categorías o géneros del libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:ratingscount">
        <prov:label>ratingscount</prov:label>
        <prov:type>xsd:decimal</prov:type>
        <dcterms:description>Promedio de calificaciones.</dcterms:description>
    </prov:entity>

    <!-- Actividad -->
    <prov:activity prov:id="ex:generate_l1books"
                   prov:startTime="2025-10-23T13:30:00"
                   prov:endTime="2025-10-23T13:37:00">
        <prov:label>Generar la tabla l1books</prov:label>
        <dcterms:description>
            Proceso de importación del dataset Amazon Books Reviews en PostgreSQL,
            generando la tabla base para el pipeline de integración.
        </dcterms:description>
    </prov:activity>

    <!-- Agentes -->
    <prov:agent prov:id="ex:integra2">
        <prov:label>Grupo Integra2</prov:label>
        <prov:type>prov:Organization</prov:type>
        <prov:role>Responsable de la integración y limpieza de los datasets.</prov:role>
    </prov:agent>

    <prov:agent prov:id="ex:postgresql_db">
        <prov:label>PostgreSQL Engine</prov:label>
        <prov:type>prov:SoftwareAgent</prov:type>
        <prov:role>Ejecutó la carga e importación de los datos.</prov:role>
    </prov:agent>

    <!-- Relaciones -->
    <prov:wasGeneratedBy prov:entity="ex:l1books"
                         prov:activity="ex:generate_l1books"/>
    <prov:wasAssociatedWith prov:activity="ex:generate_l1books"
                            prov:agent="ex:integra2"/>
    <prov:wasAssociatedWith prov:activity="ex:generate_l1books"
                            prov:agent="ex:postgresql_db"/>

</prov:document>
