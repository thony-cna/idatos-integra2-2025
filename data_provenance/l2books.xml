<?xml version="1.0" encoding="UTF-8"?>
<prov:document xmlns:prov="http://www.w3.org/ns/prov#"
               xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <!-- Entidad: Tabla l2books -->
    <prov:entity prov:id="ex:l2books">
        <prov:label>Tabla l2books</prov:label>
        <prov:description>Tabla derivada de Books Dataset, con metadatos principales de libros incluyendo título, autor, año de publicación, editorial y URLs de imágenes.</prov:description>
        <prov:creationDate>fecha</prov:creationDate>
        <prov:source>
            <prov:url>https://www.kaggle.com/datasets/saurabhbagchi/books-dataset</prov:url>
            <prov:downloadDate>2025-09-02T18:39:21</prov:downloadDate>
        </prov:source>
        <prov:columns>
            <prov:column>
                <prov:name>isbn</prov:name>
                <prov:type>xsd:string</prov:type>
                <prov:reason>Identificador único del libro (ISBN).</prov:reason>
            </prov:column>
            <prov:column>
                <prov:name>Book-Title</prov:name>
                <prov:type>xsd:string</prov:type>
                <prov:reason>Título del libro.</prov:reason>
            </prov:column>
            <prov:column>
                <prov:name>Book-Author</prov:name>
                <prov:type>xsd:string</prov:type>
                <prov:reason>Autor del libro.</prov:reason>
            </prov:column>
            <prov:column>
                <prov:name>Year-Of-Publication</prov:name>
                <prov:type>xsd:gYear</prov:type>
                <prov:reason>Año de publicación del libro.</prov:reason>
            </prov:column>
            <prov:column>
                <prov:name>publisher</prov:name>
                <prov:type>xsd:string</prov:type>
                <prov:reason>Editorial del libro.</prov:reason>
            </prov:column>
            <prov:column>
                <prov:name>Image-URL-S</prov:name>
                <prov:type>xsd:anyURI</prov:type>
                <prov:reason>URL de imagen pequeña del libro.</prov:reason>
            </prov:column>
            <prov:column>
                <prov:name>Image-URL-M</prov:name>
                <prov:type>xsd:anyURI</prov:type>
                <prov:reason>URL de imagen mediana del libro.</prov:reason>
            </prov:column>
            <prov:column>
                <prov:name>Image-URL-L</prov:name>
                <prov:type>xsd:anyURI</prov:type>
                <prov:reason>URL de imagen grande del libro.</prov:reason>
            </prov:column>
        </prov:columns>
    </prov:entity>

    <!-- Actividad: Creación de l2books -->
    <prov:activity prov:id="ex:generate_l2books"
                   prov:startTime="2025-10-23T13:30:00"
                   prov:endTime="2025-10-23T13:37:00">
        <prov:label>Generar la tabla de l2books</prov:label>
        <prov:description>Importar los datos del dataset de Books Dataset desde el CSV, cargandolos en una BD de postgresql</prov:description>
    </prov:activity>

    <!-- Agentes involucrados -->
    <prov:agent prov:id="ex:integra2">
        <prov:label>Grupo Integra2</prov:label>
        <prov:type>prov:Organization</prov:type>
        <prov:role>Responsable de la extracción y transformación de datos para l2books.</prov:role>
    </prov:agent>
    <prov:agent prov:id="ex:postgresql_db">
        <prov:label>Postgresql Import</prov:label>
        <prov:type>prov:SoftwareAgent</prov:type>
        <prov:role>Herramienta para cargar los datos</prov:role>
    </prov:agent>

    <!-- Relaciones -->
    <prov:wasGeneratedBy prov:entity="ex:l2books"
                         prov:activity="ex:generate_l2books"/>
    <prov:wasAssociatedWith prov:activity="ex:generate_l2books"
                            prov:agent="ex:integra2"/>
    <prov:wasAssociatedWith prov:activity="ex:generate_l2books"
                            prov:agent="ex:postgresql_db"/>
</prov:document>
