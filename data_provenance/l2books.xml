<?xml version="1.0" encoding="UTF-8"?>
<prov:document xmlns:prov="http://www.w3.org/ns/prov#"
               xmlns:dcterms="http://purl.org/dc/terms/"
               xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
               xmlns:ex="http://example.org/">

    <!-- Entidad principal: Tabla l2books -->
    <prov:entity prov:id="ex:l2books">
        <prov:label>Tabla l2books</prov:label>
        <prov:type>prov:Collection</prov:type>
        <dcterms:description>
            Tabla derivada de Books Dataset, con metadatos principales de libros incluyendo título, autor, año de publicación, editorial y URLs de imágenes.
        </dcterms:description>
        <dcterms:source>https://www.kaggle.com/datasets/saurabhbagchi/books-dataset</dcterms:source>
        <dcterms:date>2025-09-02T18:39:21</dcterms:date>

        <!-- Miembros (columnas) -->
        <prov:hadMember prov:entity="ex:isbn"/>
        <prov:hadMember prov:entity="ex:Book-Title"/>
        <prov:hadMember prov:entity="ex:Book-Author"/>
        <prov:hadMember prov:entity="ex:Year-Of-Publication"/>
        <prov:hadMember prov:entity="ex:publisher"/>
        <prov:hadMember prov:entity="ex:Image-URL-S"/>
        <prov:hadMember prov:entity="ex:Image-URL-M"/>
        <prov:hadMember prov:entity="ex:Image-URL-L"/>
    </prov:entity>

    <!-- Columnas -->
    <prov:entity prov:id="ex:isbn">
        <prov:label>isbn</prov:label>
        <prov:type>xsd:string</prov:type>
        <dcterms:description>Identificador único del libro (ISBN).</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:Book-Title">
        <prov:label>Book-Title</prov:label>
        <prov:type>xsd:string</prov:type>
        <dcterms:description>Título del libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:Book-Author">
        <prov:label>Book-Author</prov:label>
        <prov:type>xsd:string</prov:type>
        <dcterms:description>Autor del libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:Year-Of-Publication">
        <prov:label>Year-Of-Publication</prov:label>
        <prov:type>xsd:gYear</prov:type>
        <dcterms:description>Año de publicación del libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:publisher">
        <prov:label>publisher</prov:label>
        <prov:type>xsd:string</prov:type>
        <dcterms:description>Editorial del libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:Image-URL-S">
        <prov:label>Image-URL-S</prov:label>
        <prov:type>xsd:anyURI</prov:type>
        <dcterms:description>URL de imagen pequeña del libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:Image-URL-M">
        <prov:label>Image-URL-M</prov:label>
        <prov:type>xsd:anyURI</prov:type>
        <dcterms:description>URL de imagen mediana del libro.</dcterms:description>
    </prov:entity>

    <prov:entity prov:id="ex:Image-URL-L">
        <prov:label>Image-URL-L</prov:label>
        <prov:type>xsd:anyURI</prov:type>
        <dcterms:description>URL de imagen grande del libro.</dcterms:description>
    </prov:entity>

    <!-- Actividad: Creación de l2books -->
    <prov:activity prov:id="ex:generate_l2books"
                   prov:startTime="2025-10-23T13:30:00"
                   prov:endTime="2025-10-23T13:37:00">
        <prov:label>Generar la tabla de l2books</prov:label>
        <dcterms:description>
            Importar los datos del dataset de Books Dataset desde el CSV, cargándolos en una BD de PostgreSQL.
        </dcterms:description>
    </prov:activity>

    <!-- Agentes involucrados -->
    <prov:agent prov:id="ex:integra2">
        <prov:label>Grupo Integra2</prov:label>
        <prov:type>prov:Organization</prov:type>
        <prov:role>Responsable de la extracción y transformación de datos para l2books.</prov:role>
    </prov:agent>

    <prov:agent prov:id="ex:postgresql_db">
        <prov:label>PostgreSQL Import</prov:label>
        <prov:type>prov:SoftwareAgent</prov:type>
        <prov:role>Herramienta para cargar los datos.</prov:role>
    </prov:agent>

    <!-- Relaciones -->
    <prov:wasGeneratedBy prov:entity="ex:l2books"
                         prov:activity="ex:generate_l2books"/>
    <prov:wasAssociatedWith prov:activity="ex:generate_l2books"
                            prov:agent="ex:integra2"/>
    <prov:wasAssociatedWith prov:activity="ex:generate_l2books"
                            prov:agent="ex:postgresql_db"/>

</prov:document>
